{
  "env": {
      "production": {
        "plugins": [
          "transform-react-remove-prop-types"
        ]
      }
    },
    "presets": [
      "babel-preset-es2015",
      "babel-preset-es2016",
      "babel-preset-react"
    ],
    "plugins": [
      "babel-plugin-syntax-trailing-function-commas",
      "babel-plugin-transform-class-properties",
      "babel-plugin-transform-object-rest-spread",
      "babel-plugin-transform-async-to-generator",
      "babel-plugin-lodash"
    ]
}