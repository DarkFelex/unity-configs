{
  "name": "babel-preset-unity",
  "version": "0.0.5",
  "description": "Default presets of Babel configs used by Unity",
  "files": [
    ".babelrc",
    "README.md"
  ],
  "scripts": {
    "postinstall": "CWD=$(node -p 'process.cwd().split(\"node_modules\")[0]'); ln -f ./.babelrc $CWD/.babelrc"
   },
  "dependencies": {
